<?xml version="1.0"?>
<svg:svg xmlns:svg='http://www.w3.org/2000/svg'>
	<svg:defs>
		<svg:style type="text/css">
			@import url("/public/examples/designer/designer.css");
      @import url("/public/context/editor/editor.css");
		</svg:style>

	</svg:defs>
	<diagram xmlns="http://www.kite9.org/schema/adl"
		id="The Diagram">
		<glyph id="auto:0-one">
			<stereotype id="auto:0-one-stereo">
				Stereo
				<symbols>
					<symbol k9-shape="square">x</symbol>
					<symbol k9-shape="circle">y</symbol>
					<symbol k9-shape="diamond">z</symbol>
				</symbols>
			</stereotype>

			<label id="auto:0-one-label">One</label>

			<text-lines>
				<text-line>Some text</text-line>
				<text-line>
					Some text taking up more space
					<symbols>
						<symbol k9-shape="hexagon">A</symbol>
					</symbols>
				</text-line>
			</text-lines>
		</glyph>
		<context id="bb">
			<glyph id="auto:1-two" rank="1">
				<stereotype id="auto:1-two-stereo">Stereo</stereotype>
				<label id="auto:1-two-label">Two</label>
			</glyph>
			<arrow id="meets" rank="2">
				<label id="auto:2">meets</label>
			</arrow>
			<arrow id="greets" rank="2">greets</arrow>
			<label>Some Grouping</label>
		</context>
		<key>
		  <boldText>This is a Diagram</boldText>
		  <bodyText>See here for an explanation of Symbols</bodyText>
		  <text-lines>
		    <text-line>Some X Thing<symbols><symbol k9-shape="square">x</symbol></symbols></text-line>
        <text-line>Some Y Thing<symbols><symbol k9-shape="circle">y</symbol></symbols></text-line>
        <text-line>Some Z Thing<symbols><symbol k9-shape="diamond">z</symbol></symbols></text-line>
		  </text-lines>
		</key>
		<link id="meets-auto:0-one" rank="3">
			<from reference="meets" />
			<to reference="auto:0-one" />
		</link>
		<link id="meets-auto:1-two" rank="4">
			<from reference="meets" />
			<to class="arrow" reference="auto:1-two" id="auto:3" />
		</link>
	</diagram>
</svg:svg>